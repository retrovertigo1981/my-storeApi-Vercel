{
  "name": "my-storeapi",
  "version": "1.0.0",
  "description": "Proyecto para aprender desarrollo backend con Node.js y Express.js, con el cual se desarrollarán rutas, servidores y middlewares de Express.js, construcción de una API, manipulación de errores, validación de datos y deploy.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon api/index.js",
    "start": "npm run migratiosn:run && node api/index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [],
  "author": "Sebastián Peña <pjfanchile@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@hapi/boom": "^10.0.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.12.1",
    "mysql2": "^3.9.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.2",
    "uuid": "^9.0.1",
    "vercel": "^34.1.1"
  }
}
